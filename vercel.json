{
  "version": 2,
  "builds": [
    {
      "src": "src/vercel-adapter.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/vercel-adapter.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/vercel-adapter.ts": {
      "maxDuration": 60
    }
  }
}